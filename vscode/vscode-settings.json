{
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true, "source.fixAll.stylelint": true },
  "editor.fontFamily": "Monolisa Nerd Font",
  "editor.fontLigatures": false,
  "catppuccin.italicComments": false,
  "catppuccin.italicKeywords": false,
  "editor.codeLensFontFamily": "Monolisa Nerd Font",
  "debug.console.fontFamily": "Monolisa",
  "multiCommand.commands": [
    {
      "command": "multiCommand.bigClose",
      "sequence": [
        "workbench.action.closeSidebar",
        "workbench.action.closePanel",
        "workbench.action.closeAuxiliaryBar"
      ]
    }
  ],
  "[css]": { "editor.defaultFormatter": "vscode.css-language-features" },
  "[html]": { "editor.defaultFormatter": "vscode.html-language-features" },
  "[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[jsonc]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[scss]": { "editor.defaultFormatter": "vscode.css-language-features" },
  "[vue]": { "editor.defaultFormatter": "Vue.volar" },
  "eslint.probe": [ "javascript", "javascriptreact", "typescript", "typescriptreact", "html", "vue", "markdown", "css", "scss" ],
  "editor.hover.delay": 0,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.tabCompletion": "on",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "keyboard.touchbar.enabled": false,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "window.titleBarStyle": "custom",
  "window.title": "",
  "debug.toolBarLocation": "docked",
  "breadcrumbs.symbolPath": "last",
  "volar.completion.preferredAttrNameCase": "kebab",
  "volar.diagnostics.delay": 0,
  "editor.lineNumbers": "off",
  "explorer.autoReveal": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.folding": true,
  "editor.renderLineHighlight": "none",
  "editor.suggest.showWords": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "editor.wordBasedSuggestions": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.lineHeight": 30,
  "editor.fontSize": 16,
  "debug.enableStatusBarColor": false,
  "editor.accessibilitySupport": "off",
  "editor.linkedEditing": false,
  "editor.occurrencesHighlight": false,
  "editor.selectionHighlight": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.unfoldOnClickAfterEndOfLine": false,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.scrollByPage": false,
  "editor.showFoldingControls": "never",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.glyphMargin": false,
  "[astro]": { "editor.defaultFormatter": "astro-build.astro-vscode" },
  "[postcss]": { "editor.defaultFormatter": "stylelint.vscode-stylelint" },
  "editor.guides.indentation": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlayHints.padding": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lightbulb.enabled": false,
  "files.saveConflictResolution": "overwriteFileOnDisk",
  "files.trimTrailingWhitespace": true,
  "files.simpleDialog.enable": true,
  "editor.unicodeHighlight.allowedCharacters": { "â€™": true },
  "editor.bracketPairColorization.enabled": false,
  "editor.detectIndentation": false,
  "workbench.editor.splitInGroupLayout": "vertical",
  "editor.multiCursorModifier": "ctrlCmd",
  "less.hover.references": false,
  "comments.openView": "file",
  "editor.suggestSelection": "recentlyUsed",
  "editor.suggest.localityBonus": false,
  "editor.formatOnType": false,
  "git.confirmSync": false,
  "problems.autoReveal": true,
  "problems.decorations.enabled": false,
  "outline.problems.badges": false,
  "outline.problems.colors": false,
  "outline.problems.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "csharp.referencesCodeLens.enabled": false,
  "editor.codeActionWidget.showHeaders": false,
  "editor.guides.highlightActiveBracketPair": false,
  "scm.showActionButton": false,
  "git.showCommitInput": false,
  "scm.diffDecorationsGutterWidth": 2,
  "workbench.layoutControl.enabled": false,
  "explorer.openEditors.visible": 0,
  "diffEditor.renderIndicators": false,
  "git.decorations.enabled": false,
  "editor.codeLens": false,
  "volar.inlayHints.eventArgumentInInlineHandlers": false,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.sideBar.location": "right",
  "gitlens.currentLine.enabled": false,
  "diffEditor.renderSideBySide": false,
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": false,
}
